{
  "name": "temp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ext js,jsx,ts,tsx --cache",
    "style": "prettier --loglevel warn --ignore-unknown --no-error-on-unmatched-pattern",
